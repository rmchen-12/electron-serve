{
  "name": "electron-serve",
  "version": "1.0.0",
  "description": "桌面端的后端服务",
  "private": false,
  "egg": {
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-electron-serve",
    "stop": "egg-scripts stop --title=egg-server-electron-serve",
    "dev": "cross-env DEBUG=app* egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "m:new": "sequelize migration:create",
    "m:up": "sequelize db:migrate",
    "m:down": "sequelize db:migrate:undo",
    "m:seednew": "sequelize seed:generate --name",
    "m:seedup": "sequelize db:seed:all"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "chalk": "^4.1.0",
    "debug": "^4.1.1",
    "egg": "^2.15.1",
    "egg-cors": "^2.2.3",
    "egg-jwt": "^3.1.7",
    "egg-passport": "^2.1.1",
    "egg-passport-github": "^1.0.0",
    "egg-passport-gitlab": "^1.0.1",
    "egg-passport-local": "^1.2.1",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.11.0",
    "egg-sequelize": "^5.2.1",
    "egg-y-validator": "^1.4.0",
    "koa-basic-auth": "^4.0.0",
    "mysql2": "^2.1.0",
    "passport-gitlab2": "^5.0.0",
    "ramda": "^0.27.0",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "cross-env": "^7.0.2",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "rmchen-12",
  "license": "MIT"
}
